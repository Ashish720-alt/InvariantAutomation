(set-logic HORN)
(declare-fun |inv| (Int Int) Bool)
(assert (forall ((i Int) (k Int) (ip Int) (kp Int)) 
        (=> (and (= i 0) (>= k 0) (<= k 10))
            (inv i k))))
(assert (forall ((i Int) (k Int) (ip Int) (kp Int)) 
        (=> (and (inv i k) (< i (* 1000000 k)) (= ip (+ i k)) (= kp k))
            (inv ip kp))))
(assert (forall ((i Int) (k Int) (ip Int) (kp Int)) 
        (=> (and (>= i (* 1000000 k)) (inv i k)) 
            (= i (* 1000000 k)))))
(check-sat)
(get-model)
