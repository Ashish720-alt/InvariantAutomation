(set-logic HORN)
(declare-fun |inv| (Int Int Int) Bool)
(assert (forall ((x Int) (y Int) (n Int) (xp Int) (yp Int) (np Int)) (=> (and (>= x 0) (<= (+ x (* -1 y)) 0) (< (+ y (* -1 n)) 0)) (inv x y n))))
(assert (forall ((x Int) (y Int) (n Int) (xp Int) (yp Int) (np Int)) (=> (and (inv x y n) (< (+ x (* -1 n)) 0) (> (+ x (* -1 y)) -1)  (and (= xp (+ x 1)) (= yp (+ y 1)) (= np n))) (inv xp yp np))))
(assert (forall ((x Int) (y Int) (n Int) (xp Int) (yp Int) (np Int)) (=> (and (inv x y n) (< (+ x (* -1 n)) 0) (<= (+ x (* -1 y)) -1) (not (or  (> (+ x (* -1 y)) -1))) (and (= xp (+ x 1)) (= yp y) (= np n))) (inv xp yp np))))
(assert (forall ((x Int) (y Int) (n Int) (xp Int) (yp Int) (np Int)) (=> (and (not (< (+ x (* -1 n)) 0)) (inv x y n)) (= (+ y (* -1 n)) 0))))
(check-sat)
(get-model)