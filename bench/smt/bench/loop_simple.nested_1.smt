(set-logic HORN)
(declare-fun |inv| (Int) Bool)
(assert (forall ((i Int) (ip Int)) (=> (= i 0) (inv i))))
(assert (forall ((i Int) (ip Int)) (=> (and (inv i) (< i 6)   (and (= ip (+ i 1)))) (inv ip))))
(assert (forall ((i Int) (ip Int)) (=> (and (not (< i 6)) (inv i)) (<= i 6))))
(check-sat)
(get-model)