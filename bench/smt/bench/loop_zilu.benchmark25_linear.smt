(set-logic HORN)
(declare-fun |inv| (Int) Bool)
(assert (forall ((x Int) (xp Int)) (=> (< x 0) (inv x))))
(assert (forall ((x Int) (xp Int)) (=> (and (inv x) (< x 10)   (and (= xp (+ x 1)))) (inv xp))))
(assert (forall ((x Int) (xp Int)) (=> (and (not (< x 10)) (inv x)) (= x 10))))
(check-sat)
(get-model)