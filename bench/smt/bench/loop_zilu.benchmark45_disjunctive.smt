(set-logic HORN)
(declare-fun |inv| (Int Int) Bool)
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (or (> y 0) (> x 0)) (inv x y))))
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (inv x y)  (> x 0)  (and (= xp (+ x 1)) (= yp y))) (inv xp yp))))
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (inv x y)  (<= x 0) (not (or  (> x 0))) (and (= xp x) (= yp (+ y 1)))) (inv xp yp))))
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (inv x y) (or (> x 0) (> y 0)))))
(check-sat)
(get-model)