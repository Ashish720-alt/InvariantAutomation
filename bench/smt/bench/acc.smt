(set-logic HORN)
(declare-fun |inv| (Int Int) Bool)
(assert (forall ((x Int) (y Int) (xp Int)) 
        (=> (and (= x 1) (= y 0))
            (inv x y))))
(assert (forall ((x Int) (y Int) (xp Int)) 
        (=> (and (inv x y) (= xp (+ x y)))
            (inv xp y))))
(assert (forall ((x Int) (y Int) (xp Int)) 
        (=> (inv x y) 
            (>= x y))))
(check-sat)
(get-model)
