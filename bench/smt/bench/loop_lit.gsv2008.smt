(set-logic HORN)
(declare-fun |inv| (Int Int) Bool)
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (= x -50) (> y 1000) (< y 1000000)) (inv x y))))
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (inv x y) (< x 0)   (and (= xp (+ x y)) (= yp (+ y 1)))) (inv xp yp))))
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (not (< x 0)) (inv x y)) (or (> y 0) (< x 0)))))
(check-sat)
(get-model)