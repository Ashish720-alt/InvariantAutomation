(set-logic HORN)
(declare-fun |inv| (Int Int) Bool)
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (= (+ x (* -4 y)) 0) (>= x 0)) (inv x y))))
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (inv x y) (> x 0)   (and (= xp (+ x -4)) (= yp (+ y -1)))) (inv xp yp))))
(assert (forall ((x Int) (y Int) (xp Int) (yp Int)) (=> (and (not (> x 0)) (inv x y)) (>= y 0))))
(check-sat)
(get-model)